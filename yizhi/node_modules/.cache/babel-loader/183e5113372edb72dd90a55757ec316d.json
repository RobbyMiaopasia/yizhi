A;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,QAAA,EAAA,kBAAA,MAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,MADA;AAEA,YAAA,OAAA,oBAAA,MAAA;AAFA,WAAA;AAIA;AAPA,OAAA;AASA,KAvGA;AAwGA,IAAA,YAxGA,0BAwGA;AACA,WAAA,MAAA,CAAA,YAAA,EAAA,MAAA,EAAA;AACA,QAAA,iBAAA,EAAA;AADA,OAAA;AAGA,KA5GA;AA6GA,IAAA,cA7GA,4BA6GA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,oBAAA,MAAA,CAAA,YAAA,CAAA,QAAA,IAAA,MAAA,CAAA,YAAA,CAAA,eAAA,EAAA;AACA,sBAAA,MAAA,CAAA,YAAA,CAAA,QAAA,IAAA,MAAA,CAAA,YAAA,CAAA,eAAA,EAAA;AACA,oBAAA,MAAA,CAAA,YAAA,CAAA,aAAA,GAAA,IAAA;AACA;AACA;;AALA,sBAOA,MAAA,CAAA,YAAA,CAAA,SAAA,IAAA,IAAA,IACA,MAAA,CAAA,YAAA,CAAA,aAAA,IAAA,IADA,IAEA,MAAA,CAAA,YAAA,CAAA,OATA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAWA,KAAA,CAAA;AACA,kBAAA,MAAA,EAAA,MADA;AAEA,kBAAA,GAAA,EAAA,WAFA;AAGA,kBAAA,IAAA,EAAA,MAAA,CAAA;AAHA,iBAAA,CAAA,CAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,uBAAA,aAAA,GAAA,GAAA,CAAA,IAAA;;AACA,sBAAA,KAAA,aAAA,IAAA,IAAA,EAAA;AACA,yBAAA,eAAA;AACA,oBAAA,YAAA,GAAA,KAAA;AACA,mBAHA,MAGA;AACA,yBAAA,YAAA;AACA;AACA,iBAbA,EAcA,KAdA,CAcA,UAAA,GAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,iBAhBA,CAXA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA6BA;AA1IA;AAxBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- Form -->\r\n    <div class=\"login-box\">\r\n      <el-dialog\r\n        title=\"登录\"\r\n        width=\"25%\"\r\n        top=\"30vh\"\r\n        :visible.sync=\"dialogFormVisible\"\r\n      >\r\n        <el-form :model=\"loginForm\">\r\n          <el-form-item label=\"\">\r\n            <el-input\r\n              type=\"text\"\r\n              v-model=\"loginForm.username\"\r\n              autocomplete=\"off\"\r\n              placeholder=\"请输入账号\"\r\n              @blur=\"loginJudge\"\r\n              @focus=\"focus\"\r\n            >\r\n              <template slot=\"prepend\">帐号</template>{{ loginForm.username }}\r\n            </el-input>\r\n          </el-form-item>\r\n          <p class=\"sign\" v-text=\"signMsg\">\r\n            {{ signMsg }}\r\n          </p>\r\n          <el-form-item label=\"\">\r\n            <el-input\r\n              type=\"password\"\r\n              v-model=\"loginForm.password\"\r\n              autocomplete=\"off\"\r\n              placeholder=\"请输入密码\"\r\n            >\r\n              <template slot=\"prepend\">密码</template>\r\n            </el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogFormVisible = false\">取消</el-button>\r\n          <el-button type=\"primary\" @click=\"innerVisible = true\"\r\n            >注册</el-button\r\n          >\r\n          <el-button type=\"primary\" @click=\"loginSubmit()\">登录</el-button>\r\n        </div>\r\n\r\n        <!-- 注册 -->\r\n        <el-dialog\r\n          width=\"25%\"\r\n          title=\"注册\"\r\n          :visible.sync=\"innerVisible\"\r\n          append-to-body\r\n        >\r\n          <el-form :model=\"registerForm\">\r\n            <el-form-item label=\"手机号\">\r\n              <el-input\r\n                v-model=\"registerForm.username\"\r\n                type=\"text\"\r\n                placeholder=\"请输入手机号\"\r\n                @blur=\"registerJudge\"\r\n                @focus=\"focus\"\r\n              ></el-input>\r\n            </el-form-item>\r\n            <p class=\"sign\" v-text=\"signMsg\">\r\n              {{ signMsg }}\r\n            </p>\r\n            <el-form-item label=\"密码\">\r\n              <el-input\r\n                v-model=\"registerForm.password\"\r\n                type=\"password\"\r\n                placeholder=\"请输入密码\"\r\n              ></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"确认密码\">\r\n              <el-input\r\n                v-model=\"registerForm.confirmPassword\"\r\n                type=\"password\"\r\n                placeholder=\"确认密码\"\r\n              ></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"所属单位（公司/学校）\">\r\n              <el-input\r\n                v-model=\"registerForm.company\"\r\n                type=\"text\"\r\n                placeholder=\"填入你的所属单位\"\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-form>\r\n          <div slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"innerVisible = false\">取消</el-button>\r\n            <el-button type=\"primary\" @click=\"registerSubmit()\"\r\n              >注册账号</el-button\r\n            >\r\n          </div>\r\n        </el-dialog>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      dialogTableVisible: false,\r\n      dialogFormVisible: false,\r\n      outerVisible: false,\r\n      innerVisible: false,\r\n      loginState: false,\r\n      registerState: false,\r\n      signMsg: \"\",\r\n      loginForm: {\r\n        username: \"\",\r\n        password: \"\",\r\n      },\r\n      registerForm: {\r\n        username: \"\",\r\n        password: \"\",\r\n        confirmPassword: \"\",\r\n        company: \"\",\r\n        nameState: false,\r\n        passwordState: false,\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    Dshow() {\r\n      this.dialogFormVisible = true;\r\n    },\r\n    loginJudge() {\r\n      var val = this.loginForm.username;\r\n      if (val) {\r\n        var re1 = /^14[4-9]\\d{8}/;\r\n        var re2 = /^15[0-3]|15[5-9]\\d{8}/;\r\n        var re3 = /^16[6,7]\\d{8}/;\r\n        var re4 = /^17[0,1]\\d{8}|17[3-8]\\d{8}/;\r\n        var re5 = /^1[8,9]\\d{9}/;\r\n        var re6 = /^13\\d{9}/;\r\n        if (\r\n          re1.test(val) ||\r\n          re2.test(val) ||\r\n          re3.test(val) ||\r\n          re4.test(val) ||\r\n          re5.test(val) ||\r\n          re6.test(val)\r\n        ) {\r\n          this.signMsg = \"\";\r\n        } else {\r\n          this.signMsg = \"请输入正确的手机号格式\";\r\n        }\r\n      } else {\r\n        this.signMsg = \"\";\r\n      }\r\n    },\r\n    registerJudge() {\r\n      var val = this.registerForm.username;\r\n      if (val) {\r\n        var re1 = /^14[4-9]\\d{8}/;\r\n        var re2 = /^15[0-3]|15[5-9]\\d{8}/;\r\n        var re3 = /^16[6,7]\\d{8}/;\r\n        var re4 = /^17[0,1]\\d{8}|17[3-8]\\d{8}/;\r\n        var re5 = /^1[8,9]\\d{9}/;\r\n        var re6 = /^13\\d{9}/;\r\n        if (\r\n          re1.test(val) ||\r\n          re2.test(val) ||\r\n          re3.test(val) ||\r\n          re4.test(val) ||\r\n          re5.test(val) ||\r\n          re6.test(val)\r\n        ) {\r\n          this.signMsg = \"\";\r\n          this.registerForm.nameState = true;\r\n        } else {\r\n          this.signMsg = \"请输入正确的手机号格式\";\r\n        }\r\n      } else {\r\n        this.signMsg = \"\";\r\n      }\r\n    },\r\n    focus() {\r\n      this.signMsg = \"\";\r\n    },\r\n    loginSuccess() {\r\n      this.$alert(\"登录成功\", \"登录\", {\r\n        confirmButtonText: \"确定\",\r\n        callback: (action) => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: `action: ${action}`,\r\n          });\r\n        },\r\n      });\r\n    },\r\n    loginFail() {\r\n      this.$alert(\"确认账号密码是否正确\", \"登录失败\", {\r\n        confirmButtonText: \"确定\",\r\n      });\r\n    },\r\n    async loginSubmit() {\r\n      await axios({\r\n        method: \"post\",\r\n        url: \"/Login\",\r\n        data: this.loginForm,\r\n      })\r\n        .then(function (res) {\r\n          this.loginState = res.data;\r\n          if (this.loginState == true) {\r\n            this.loginSuccess();\r\n            this.dialogFormVisible = false;\r\n          } else {\r\n            this.loginFail();\r\n          }\r\n        })\r\n        .catch(function (err) {\r\n          console.log(err);\r\n        });\r\n    },\r\n    registerSuccess() {\r\n      this.$alert(\"注册成功\", \"注册\", {\r\n        confirmButtonText: \"确定\",\r\n        callback: (action) => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: `action: ${action}`,\r\n          });\r\n        },\r\n      });\r\n    },\r\n    registerFail() {\r\n      this.$alert(\"请输入2-20位密码\", \"注册失败\", {\r\n        confirmButtonText: \"确定\",\r\n      });\r\n    },\r\n    async registerSubmit() {\r\n      if (this.registerForm.password && this.registerForm.confirmPassword) {\r\n        if (this.registerForm.password == this.registerForm.confirmPassword) {\r\n          this.registerForm.passwordState = true;\r\n        }\r\n      }\r\n      if (\r\n        this.registerForm.nameState == true &&\r\n        this.registerForm.passwordState == true &&\r\n        this.registerForm.company\r\n      ) {\r\n        await axios({\r\n          method: \"post\",\r\n          url: \"/register\",\r\n          data: this.registerForm,\r\n        })\r\n          .then(function (res) {\r\n            this.registerState = res.data;\r\n            if (this.registerState == true) {\r\n              this.registerSuccess();\r\n              innerVisible = false;\r\n            } else {\r\n              this.registerFail();\r\n            }\r\n          })\r\n          .catch(function (err) {\r\n            console.log(err);\r\n          });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\">\r\n.sign {\r\n  color: rgb(245, 57, 57);\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}